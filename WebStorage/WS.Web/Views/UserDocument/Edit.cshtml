@model WS.Business.ViewModels.UserDocumentView
@{
    ViewBag.Title = "Редактирование";
}
<form asp-action="edit" asp-controller="UserDocument" asp-route-id1="@Model.GuestEmail" asp-route-id2="@Model.DocumentId">
    <div class="form-group">
        <label asp-for="GuestEmail" class="control-label">User_id</label>
        <input type="text" asp-for="GuestEmail" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DocumentId" class="control-label">Document_id</label>
        <input type="text" asp-for="DocumentId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Link" class="control-label">Link</label>
        <input type="number" asp-for="Link" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="IsEditable" class="control-label">IsEditable</label>
        <input type="number" asp-for="IsEditable" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Send" class="btn btn-default" />
    </div>
</form>